<%= render "review_verification_filter", q: @q, branch_options: @support.branch_options %>
<table class="table">
  <thead>
    <th><%= t ".index" %></th>
    <th><%= ReviewVerification.human_attribute_name :email %></th>
    <th><%= ReviewVerification.human_attribute_name :phone_number %></th>
    <th><%= ReviewVerification.human_attribute_name :status %></th>
    <th><%= t ".branch_name" %></th>
    <th><%= t ".action" %></th>
  </thead>
  <tbody id="review-verification-list">
    <%= render partial: "review_verification", collection: @review_verifications,
      as: :review_verification %>
  </tbody>
</table>
<div id="review-verification-pagination">
  <%= paginate @review_verifications %>
</div>
