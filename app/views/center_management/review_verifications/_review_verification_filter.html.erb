<%= form_for q, url: center_management_review_verifications_path, method: :get do |f| %>
  <%= f.select :status_eq, review_verification_status_options, {include_blank: t(".select_status")}%>
  <%= f.select :branch_slug_eq, branch_options, {include_blank: t(".select_branch")} %>
  <%= f.submit t(".filter") %>
<% end %>
