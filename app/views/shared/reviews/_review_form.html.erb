<%=
  render partial: "shared/reviews/criterion_input", collection: criterion_list,
    as: :criterion, locals: {f: f, options: rating_values}
%>

<div class="form-group">
  <%= f.label :title, Review.human_attribute_name(:title) %><br/>
  <%= f.text_field :title %>
</div>

<div class="form-group">
  <%= f.label :content, Review.human_attribute_name(:content) %><br/>
  <%= f.text_area :content %>
</div>
