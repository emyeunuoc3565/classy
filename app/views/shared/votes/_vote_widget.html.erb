<div class="col col-md-1 vote-widget">
  <div class="panel panel-default text-center">
    <div class="panel-heading">
      <%= render "shared/votes/vote_button", review: review, vote_type: :up %>
    </div>
    <div class="panel-body"><%= review.vote_points_cached %></div>
    <div class="panel-footer">
      <%= render "shared/votes/vote_button", review: review, vote_type: :down %>
    </div>
  </div>
</div>
