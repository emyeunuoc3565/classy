var position = new google.maps.LatLng("<%= branch.coordinates.x %>", "<%= branch.coordinates.y %>");
var mapOptions = {
  mapTypeId: "roadmap",
  zoom: <%= Settings.google_map.branch.zoom %>,
  center: position
};
var map = new google.maps.Map(document.getElementById("map_canvas"), mapOptions);
var marker = new google.maps.Marker({
  position: position,
  map: map,
  title: "<%= branch.name %>",
  label: "<%= branch.cached_avarage_rating %>",
  icon: "<%= Settings.google_map.branch_info.icon_url %>"
});
